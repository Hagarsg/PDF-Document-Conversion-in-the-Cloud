
                        "# Capture the PID of the background process\n" +
                        "MANAGER_PID=$!\n" +
                        "\n" +
                        "# Wait for a moment to ensure the manager is running\n" +
                        "sleep 2\n" +
                        "\n" +
                        "# Check if the manager process is running\n" +
